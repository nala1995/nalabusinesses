{
    "version": 2,
    "ignoreDuringBuilds": [
    "eslint"
  ],
    "builds": [
      {
        "src": "server.cjs",
        "use": "@vercel/node"
      },
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/server.cjs"
      }
    ]
  }
  